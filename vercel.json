{
  "version": 2,
  "builds": [
    {
      "src": "app/src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "app/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "app/src/server.js"
    },
    {
      "src": "/app",
      "dest": "app/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "app/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}